public class Solution {
    public bool IsPalindrome(int x) {
        if (x < 0 ){
            return false;
        }
            
        string snum = x.ToString();
        int r = snum.Length -1;    
        for(int i=0;i<=(snum.Length/2);i++){
            if(snum[i] != snum[r]) return false;
            r--;
        }
        return true;  
    }
}
