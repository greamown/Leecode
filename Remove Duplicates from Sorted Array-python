class Solution:
    def removeDuplicates(self, nums: List[int]) -> int:
        if len(nums)==0:
            return 0
        intx = 0
        for i in range(1,len(nums)):
            if nums[i] != nums[intx]:
                intx += 1
                nums[intx] = nums[i]
                
        return intx+1
