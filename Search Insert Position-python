class Solution:
    def searchInsert(self, nums: List[int], target: int) -> int:
        small, big = 0,len(nums)-1
        mid = (small+big)//2
        while small <= big:
            if nums[mid] == target:
                return mid
            elif nums[mid] > target:
                big = mid -1
            else:
                small = mid +1
            
            mid = (small+big)//2
            
        return small
